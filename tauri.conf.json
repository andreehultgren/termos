{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "dist",
    "distDir": "dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Termos",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.termos.app"
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Termos",
        "width": 1200,
        "height": 800
      }
    ]
  }
}
